{
  "run_list": [
    "role[packer]",
    "recipe[php]",
    "role[web-nginx]",
    "recipe[config-driven-helper::packages]"
  ],
  "nginx": {
    "sites": {
      "{{hostname}}": {
        "docroot": "/vagrant/public",
        "php-fpm": {
          "listen": "tcp",
          "host": "127.0.0.1",
          "port": "9000"
        }
      }
    }
  }
}
